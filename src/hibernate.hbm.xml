<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.hibenate.dto.CustomerEntity" table="customers">
		<id name="customerid" type="java.lang.String">
			<column name="CustomerId"></column>
		</id>
		<property name="customerName">
			<column name="CustomerName"></column>
		</property>
		<property name="contactName">
			<column name="ContactName"></column>
		</property>
		<property name="address">
			<column name="Address"></column>
		</property>
		<property name="city">
			<column name="City"></column>
		</property>
		<property name="postalCode">
			<column name="PostalCode"></column>
		</property>
		<property name="country">
			<column name="Country"></column>
		</property>
	</class>
</hibernate-mapping>